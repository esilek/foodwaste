<script>
    $('.hides').hide();

    function clearfunc() {
        $('.hideimg').show();
        document.getElementById("p1").innerHTML = ""
        $('.hides').hide();
    }

    $('input[value="Banana"]').click(function() {
        $('.hideimg').hide();
        $('.bananaimg').show();
        document.getElementById("p1").innerHTML = "You have Bananas"

    });

    $('input[value="Pumpkin"]').click(function() {
        $('.hideimg').hide();
        $('.pumpkinimg').show();
        document.getElementById("p1").innerHTML = "You have Pumpkin"
    });
    $('input[value="Bread"]').click(function() {
        $('.hideimg').hide();
        $('.breadimg').show();
        document.getElementById("p1").innerHTML = "You have Bread"
    });

    $('input[value="Milk"]').click(function() {
        $('.hideimg').hide();
        $('.milkimg').show();
        document.getElementById("p1").innerHTML = "You have Milk"
    });
    $('input[value="Potatoes"]').click(function() {
        $('.hideimg').hide();
        $('.potatoimg').show();
        document.getElementById("p1").innerHTML = "You have Potatoes"
    });
    $('input[value="Tomatoes"]').click(function() {
        $('.hideimg').hide();
        $('.tomatoimg').show();
        document.getElementById("p1").innerHTML = "You have Tomatoes"
    });
    $('input[value="Tomatoes"]').click(function() {
        $('.hideimg').hide();
        $('.tomatoimg').show();
        document.getElementById("p1").innerHTML = "You have Tomatoes"
    });
    $('input[value="Strawberries"]').click(function() {
        $('.hideimg').hide();
        $('.strawberryimg').show();
        document.getElementById("p1").innerHTML = "You have Strawberries"
    });
    $('input[value="Broccoli"]').click(function() {
        $('.hideimg').hide();
        $('.brocolliimg').show();
        document.getElementById("p1").innerHTML = "You have Brocolli"
    });
    $('input[value="Eggs"]').click(function() {
        $('.hideimg').hide();
        $('.eggimg').show();
        document.getElementById("p1").innerHTML = "You have Eggs"
    });
    $('input[value="Eggs"]').click(function() {
        $('.hideimg').hide();
        $('.eggimg').show();
        document.getElementById("p1").innerHTML = "You have Eggs"
    });
    $('input[value="Cheese"]').click(function() {
        $('.hideimg').hide();
        $('.cheeseimg').show();
        document.getElementById("p1").innerHTML = "You have Cheese"
    });
    $('input[value="Avocados"]').click(function() {
        $('.hideimg').hide();
