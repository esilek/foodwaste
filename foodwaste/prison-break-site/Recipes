    });

    $('.butx').hide();

    var breadR = "<br/><h3>Bread and Butter Pudding</h3><ul><li> 25g/1oz butter </li><li> Thin slices bread </li><li> 50g/2oz sultanas </li><li> 2 tsp cinnamon powder </li><li> 350ml/12fl oz whole milk </li><li> 50ml/2fl oz double cream </li><li> 2 free-range eggs </li><li> 25g/1oz granulated sugar </li></ul>"
    var buttons;

    var bananaR = "<br/><h3>Banana Cake</h3><ul><li> 25g/1oz butter </li><li> Banana </li> <li> 2 tsp cinnamon powder </li><li> 350ml/12fl oz whole milk </li><li> 50ml/2fl oz double cream </li><li> 2 free-range eggs </li><li> 25g/1oz granulated sugar </li></ul>"


    var pumpkinR = "<br/><h3>Pumpkin Soup</h3><ul><li>2 onions</li><li> 1kg pumpkin </li> <li> 700ml vegetable stock </li><li> 150ml double cream </li><li> </ul>"

    var paneerR = "<br/><h3>Paneer</h3><ul><li>2 litres/3½ pints whole milk</li><li> 2 tbsp lemon juice </li> </ul>"
   
    var potatoR = "<br/><h3>Potato Dauphinoise</h3><ul><li>  500ml double cream</li><li> 500ml milk </li> <li> 3 garlic cloves</li> <li> 8 large potatoes</li> </ul>"
    var 


  



    $('#butx' + buttons).hide();

    $('#1').click(changeimg);

    function changeimg() {
        buttons = 1;
        $('#foodimg' + buttons).addClass('imageclass2');
        $('#imgtext' + buttons).addClass('imgtextclass2');
        $('#butx' + buttons).show();
        document.getElementById("imgtext" + buttons).innerHTML = breadR;
    }

    $("#butx" + 1).click(changeback);

    function changeback() {
        buttons = 1;

        $('#foodimg' + buttons).removeClass('imageclass2');
        $('#imgtext' + buttons).removeClass('imgtextclass2');
        $('#butx' + buttons).hide();
        document.getElementById("imgtext" + buttons).innerHTML = "Bread and Butter Pudding";
    }

    $('#2').click(changeimg2);

    function changeimg2() {
        buttons = 2;
        $('#foodimg' + buttons).addClass('imageclass2');
        $('#imgtext' + buttons).addClass('imgtextclass2');
        $('#butx' + buttons).show();
        document.getElementById("imgtext" + buttons).innerHTML = bananaR;
    }

    $("#butx" + 2).click(changeback2);

    function changeback2() {
        buttons = 2

        $('#foodimg' + buttons).removeClass('imageclass2');
        $('#imgtext' + buttons).removeClass('imgtextclass2');
        $('#butx' + buttons).hide();
        document.getElementById("imgtext" + buttons).innerHTML = "Banana Cake";
    }

    $('#3').click(changeimg3);

    function changeimg3() {
        buttons = 3;
        $('#foodimg' + buttons).addClass('imageclass2');
        $('#imgtext' + buttons).addClass('imgtextclass2');
        $('#butx' + buttons).show();
        document.getElementById("imgtext" + buttons).innerHTML = pumpkinR;
    }

    $("#butx" + 3).click(changeback3);

    function changeback3() {
        buttons = 3

        $('#foodimg' + buttons).removeClass('imageclass2');
        $('#imgtext' + buttons).removeClass('imgtextclass2');
        $('#butx' + buttons).hide();
        document.getElementById("imgtext" + buttons).innerHTML = "Pumpkin Soup";
    }

    $('#4').click(changeimg4);

    function changeimg4() {
        buttons = 4;
        $('#foodimg' + buttons).addClass('imageclass2');
        $('#imgtext' + buttons).addClass('imgtextclass2');
        $('#butx' + buttons).show();
        document.getElementById("imgtext" + buttons).innerHTML = paneerR;
    }

    $("#butx4").click(changeback4);

